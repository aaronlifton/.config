<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>Label</key>
    <string>com.aaron.scrypted.autostart</string>
    <key>ProgramArguments</key>
    <array>
        <string>/Applications/Scrypted.app/Contents/MacOS/scrypted-electron</string>
    </array>
