    <!--          <string>-y</string> -->
    <!--          <string>scrypted</string> -->
    <!--          <string>serve</string> -->
    <!--     </array> -->
    <key>WorkingDirectory</key>
         <string>/Users/aaron/.scrypted</string>
    <key>StandardOutPath</key>
        <string>/dev/null</string>
    <key>StandardErrorPath</key>
        <string>/dev/null</string>
    <key>UserName</key>
        <string>aaron</string>
    <key>EnvironmentVariables</key>
        <dict>
            <key>NODE_OPTIONS</key>
                <string>--dns-result-order=ipv4first</string>
            <key>PATH</key>
                <string>/opt/homebrew/bin/:/usr/local/bin:/opt/homebrew/bin:/opt/homebrew/sbin:/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin</string>
            <key>HOME</key>
                <string>/Users/aaron</string>
            <key>SCRYPTED_PYTHON_PATH</key>
